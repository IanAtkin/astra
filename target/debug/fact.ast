; factorials!

fn factorial(n) [
    if (n == 0) [
        return 1
    ]
    return n * factorial(n - 1)
]

input = 3
print("Factorial of {} = {}\n", input, factorial(input))

input = 10
print("Factorial of {} = {}\n", input, factorial(input))

input = 20
print("Factorial of {} = {}\n", input, factorial(input))

input = 50
print("Factorial of {} = {}\n", input, factorial(input))

input = 100
print("Factorial of {} = {}\n", input, factorial(input))